function F = determine_F (constants, C_e__b_meas, f_b__i_b_meas, r_e__e_b_meas)

[L_b_meas, ~, ~] = xyz2llh(constants, r_e__e_b_meas);
g_0 = 9.7803253359 * (1 +  0.00193185*(sin(L_b)^2)) / sqrt(1 - (constants.e^2)*(sin(L_b)^2));
r_e__e_S = g_0;
delta_g_e__b = (2 * g_0 / r_e__e_S) * (r_e__e_b_meas / norm(r_e__e_b_meas)^2) * r_e__e_b_meas';

F+

F = [   -constants.Ohm_e__i_e               zer                     zer             zer     -C_e__b_meas*constants.gyro.G_g;  % F_phi_g = g-sensitivity
        - vec2ss(C_e__b_meas*f_b__i_b_meas) -2*constants.Ohm_e__i_e delta_g_e__b    -C_e__b_meas * F_va

end